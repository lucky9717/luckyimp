/*to show the eligiblity*/
window.localSelectedspec = function (a) {
    switch (a.value) {

        case 'B.Com (Hons.) Financial Markets':
            a.parentNode.parentNode.querySelector('.courseEligbility').innerHTML = "<ul class='ul-styling-class'> <li><a href='https://www.galgotiasuniversity.edu.in/admissions/fee-structure'  target='_blank'>Click to see Fee Structure</a> </li> <li>Duration: </i>3 Years</li> <li>Eligibility : </i>XII with minimum 50% marks in best four relevant subjects Mathematics/Commerce as a subject in XII level</ul>";
            break;
        case 'B.Sc. (General) Mathematics and Data Science':
            a.parentNode.parentNode.querySelector('.courseEligbility').innerHTML = "<ul class='ul-styling-class'> <li><a href='https://www.galgotiasuniversity.edu.in/admissions/fee-structure'  target='_blank'>Click to see Fee Structure</a> </li> <li>Duration: </i>3 years</li> <li>Eligibility : </i> XIIth with PCM with minimum 50% marks.</ul>";
            break;
        case 'B.Sc. (General) PCM ( Physics Chemistry and Maths)':
            a.parentNode.parentNode.querySelector('.courseEligbility').innerHTML = "<ul class='ul-styling-class'> <li><a href='https://www.galgotiasuniversity.edu.in/admissions/fee-structure'  target='_blank'>Click to see Fee Structure</a> </li> <li>Duration: </i>3 years</li> <li>Eligibility : </i>XIIth with Science with minimum 50% marks.</ul>";
